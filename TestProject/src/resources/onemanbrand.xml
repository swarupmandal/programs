<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:util="http://www.springframework.org/schema/util"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	                    http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		 				http://www.springframework.org/schema/context
		 				http://www.springframework.org/schema/context/spring-context-4.0.xsd
		 				http://www.springframework.org/schema/util
		 				http://www.springframework.org/schema/util/spring-util-4.0.xsd">






	<bean id="hank" class="org.spr.model.OneManBrand">	

		<property name="name" value="Hank"></property>

		<property name="instruments">
			<list>
				<ref bean="piano" />
				<ref bean="guiter" />
				<ref bean="tobla" />
				<ref bean="harmonium" />
			</list>
		</property>
	</bean>

	<bean id="piano" class="org.spr.model.Piano"></bean>
	<bean id="guiter" class="org.spr.model.Guiter"></bean>
	<bean id="harmonica" class="org.spr.model.Guiter"></bean>

	

</beans>